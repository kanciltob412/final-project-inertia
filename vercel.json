{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "public/build",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "resources/js/ssr.tsx": {
      "runtime": "nodejs18.x"
    }
  }
}